@model OnlineBanking.Domain.Entities.Account
@{
    ViewData["Title"] = "SignUp";
}

<!DOCTYPE html>
<html>
<head>
    <meta name=" viewport" content="width=device-width" />
    <title>ShEx Sign Up Page</title>
</head>
<body>
    <div>
        <h1>
            Sign Up For a free ShEx Bank Account
        </h1>
    </div>
    <div>
        <form asp-action="SignUp" method="post">
            <div class="form-group">
                <label asp-for="Email">Quote Author:</label>
                <input asp-for="Email" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Password">Quote:</label>
                <input asp-for="Password" class="form-control" />
            </div>
            <button type="submit" class="btn btn-primary">SUBMIT INPUT</button>
        </form>
    </div>


</body>
</html>


